    VelocityValue     = 80000, -- c∆∞·ªùng ƒë·ªô velocity
    VelocityWait      = 0.015, -- delay gi·ªØa l·∫ßn set velocity
    RotationLoops     = 50,    -- s·ªë v√≤ng xoay
